{
  "compilerOptions": {
    "target": "ES2018",
    "strict": true,
    "moduleResolution": "node",
    "module": "CommonJS",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
      "jest",
      "jest-extended"
    ],
  },
  "include": [
    "."
  ],
  "exclude": [
    "node_modules"
  ]
}
